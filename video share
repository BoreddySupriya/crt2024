#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int userpoints;
    scanf("%d",&userpoints);
    if(userpoints<=100 && userpoints>=30)
    {
        if(userpoints>=30 && userpoints<=50)
        {
            printf("Average");
        }
        else if(userpoints>=51 && userpoints<=60)
        {
            printf("Good");
        }
        else if(userpoints>=61 && userpoints<80)
        {
            printf("Excellent");
        }
        else
        {
            printf("Outstanding");
        }
            
        }
    
    else
    {
        printf("Invalid Input");
    }
    return 0;
}
