#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
   
       int sum=0;
    int num;
    scanf("%d",&num);
    while(num!=-1)
    {
        sum=sum+num;
        scanf("\n%d",&num);
    }
    printf("%d",sum);
    return 0;
}
